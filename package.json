    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "string-replace-loader": "^3.1.0",
    "supabase": "^1.142.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}

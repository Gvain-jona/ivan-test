{
  "name": "ivan-prints",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:normal": "next dev",
    "build": "next build",
    "build:prod": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "prepare": "husky",
    "clean": "node clean-build.js",
    "clean:dev": "node clean-build.js && npm install && npm run dev",
    "clean:dev:normal": "node clean-build.js && npm install && npm run dev:normal",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "supabase:reset": "npx supabase db reset",
    "supabase:seed": "npx supabase db reset && node supabase/seed.js",
    "supabase:push-to-cloud": "node scripts/push-to-cloud.js",
    "env:local": "node scripts/switch-env.js local",
    "env:cloud": "node scripts/switch-env.js cloud",
    "dev:local": "npm run env:local && npm run dev",
    "dev:cloud": "npm run env:cloud && npm run dev",
    "create-test-users": "node scripts/create-test-users.js",
    "deploy": "npm run build:prod && bash scripts/deploy.sh",
    "ui:init": "npm i lucide-react && npx shadcn-ui@latest init",
    "ui:add": "npx shadcn-ui@latest add"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
